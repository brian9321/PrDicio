<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:top="0dp">
        <shape android:width="50dp">
            <padding
                android:left="12dp"
                android:right="12dp" />
            <corners android:radius="24dp" />
            <stroke
                android:width="2dp"
                android:color="#EEEEF2" />
            <solid android:color="@android:color/white" />
        </shape>
    </item>
</layer-list>
